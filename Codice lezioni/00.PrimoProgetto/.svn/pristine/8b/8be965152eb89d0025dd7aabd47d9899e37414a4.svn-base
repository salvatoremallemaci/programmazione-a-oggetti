package ereditarieta;

public class EsempioVettore {

	public static void main(String[] args) {
		Vettore v = new Vettore(10);
		
		v.add(42);
		
		System.out.println(v.get(0));
		
		System.out.println(v.get(99));
		
		VettoreOrdinato vo = new VettoreOrdinato();
		
		vo.add(1);
		vo.add(7);
		vo.add(3);
		vo.add(2);
		
		System.out.println(vo.get(3));
		
		System.out.println(vo.toString());

	}

}
