package clinic;

public class Patient {

	private String nome;
	private String cognome;
	private String ssn;
	private Doctor dottore;

	public Patient(String nome, String cognome, String ssn) {
		super();
		this.nome = nome;
		this.cognome = cognome;
		this.ssn = ssn;
	}

	public String getNome() {
		return nome;
	}

	public String getCognome() {
		return cognome;
	}

	public String getCodiceFiscale() {
		return ssn;
	}

	public String toString() {
		return cognome + " " + nome + " " + "(" + ssn + ")";

	}

	public Doctor getDottore() {
		return dottore;
	}

	public void setDottore(Doctor dottore) {
		this.dottore = dottore;
	}

}
