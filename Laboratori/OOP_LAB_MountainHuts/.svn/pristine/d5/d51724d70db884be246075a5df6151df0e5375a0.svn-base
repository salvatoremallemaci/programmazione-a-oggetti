package mountainhuts;

public class Altitude {

	private Integer minValue;
	private Integer maxValue;
	private String 	intervallo;

	public Altitude(Integer minValue, Integer maxValue) {
		this.minValue = minValue;
		this.maxValue = maxValue;
		this.intervallo = minValue + "-" + maxValue;
	}

	public void setMinValue(Integer minValue) {
		this.minValue = minValue;
	}

	public void setMaxValue(Integer maxValue) {
		this.maxValue = maxValue;
	}

	public void setIntervallo(String intervallo) {
		this.intervallo = this.minValue + "-" + this.maxValue;
	}

	public Integer getMinValue() {
		return minValue;
	}

	public Integer getMaxValue() {
		return maxValue;
	}

	public String getIntervallo() {
		return intervallo;
	}
	
	
}
