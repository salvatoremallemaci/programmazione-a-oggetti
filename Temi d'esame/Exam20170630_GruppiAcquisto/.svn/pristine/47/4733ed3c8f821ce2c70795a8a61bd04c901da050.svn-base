package groups;

public class Bid {
	
	private String supplierName,productName;
	private Integer price;
	
	public Bid(String supplierName, String productName, int price) {
		super();
		this.supplierName = supplierName;
		this.productName = productName;
		this.price = price;
	}

	public String getSupplierName() {
		return supplierName;
	}

	public String getProductName() {
		return productName;
	}

	public Integer getPrice() {
		return price;
	}
	
	Integer nVotes = 0;

	public Integer getnVotes() {
		return nVotes;
	}
	
}
